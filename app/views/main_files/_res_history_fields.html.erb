 <fieldset>
</br>
	<div class="well">
		<%= link_to_remove_association "Remove", f, :class => "btn btn-danger" %>
		</br>
		<div class="row">
			<div class="col-md-6">
				<%= f.input :house_number, label: 'House Number' %></br>
				<%= f.input :street, label: 'Street' %></br>
				<%= f.input :dtc, label: 'District/Town/City' %></br>
			</div>
			<div class="col-md-6">
				<%= f.input :province, label: 'Province' %></br>
				<%= f.input :start_date, :as => :date, :start_year => 1910, :end_year => Date.today.year, label: 'Start Date' %></br>
				<%= f.input :end_date, :as => :date, :start_year => 1910, :end_year => Date.today.year, label: 'End Date' %></br>
			</div>
		</div>
	</div>
</fieldset>